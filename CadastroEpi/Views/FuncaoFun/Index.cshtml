@model IEnumerable<CadastroEpi.Domain.Model.Funcao>

@{
    ViewBag.Title = "Função";
}


<table class="table">
    <tr>
        
        <th>
            <label for="formGroupExampleInput" class="form-label">Função</label>
        </th>
        <th>
            <label for="formGroupExampleInput" class="form-label">Numeração do EPI</label>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        
        <td>
            @Html.DisplayFor(modelItem => item.NomeFuncao)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EpiId)
        </td>
        
    </tr>
}

</table>
